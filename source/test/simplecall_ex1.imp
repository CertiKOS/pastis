var x, y, n, z;

function nested_loop() do

focus
  max0_ge_0(x),
  max0_ge_arg(x),
  max0_le_arg(x-1),
  max0_ge_0(y),
  max0_ge_arg(y+1),
  max0_ge_0(n),
  max0_ge_0(n-y),
  max0_le_arg(n-y-1)
  ;
  y = 0;
  while y < n do
    y = y + 1;
    x = x - 1;
    z = z + 1;
  end
end

function start() do

focus
  max0_ge_0(x),
  max0_ge_arg(x),
  max0_le_arg(x-1),
  max0_ge_0(y),
  max0_ge_arg(y+1),
  max0_ge_0(n),
  max0_ge_0(n-y),
  max0_le_arg(n-y-1)
  ;

assume n > 0;

  z = 0;
  while x > 0 do
    nested_loop();
  end
end

function simple() do

assume n > 0;

  z = 0;
  while x > 0 do
    y = 0;
    while y < n do
      y = y + 1;
      x = x - 1;
      z = z + 1;
    end
  end
end

function even_simple() do

  z = 0;
  while x > 0 do
    x = x - 3;
    z = z + 1;
  end
end
